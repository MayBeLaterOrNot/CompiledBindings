<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0</TargetFrameworks>
		<RootNamespace>CompiledBindings</RootNamespace>
		<AssemblyName>CompiledBindings.WinUI</AssemblyName>
		<Product>CompiledBindings</Product>
		<Company>levitali</Company>
		<Version>1.0.16</Version>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<WarningsAsErrors>nullable</WarningsAsErrors>
		<OutputPath>..\..\..\bin\winui\build</OutputPath>
		<CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
		<NoWarn>$(NoWarn);8632</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Build.Framework" Version="17.6.3" IncludeAssets="compile" />
		<PackageReference Include="Microsoft.Build.Utilities.Core" Version="17.6.3" IncludeAssets="compile" />
		<PackageReference Include="Mono.Cecil" Version="0.11.5" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\CompiledBindings.Core\CompiledBindings.Core.csproj" />
	</ItemGroup>

</Project>