<?xml version="1.0" encoding="utf-8"?>
<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:WPFTest" xmlns:m="http://compiledbindings.com/generators" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:mx="http://compiledbindings.com/x" xmlns:viewModels="clr-namespace:WPFTest.ViewModels" mc:Ignorable="d m mx" x:Class="WPFTest.Views.MainWindow" mx:DataType="viewModels:MainViewModel" Width="800" Height="450" d:Title="WPFTest" x:Name="window1" xmlns:g0="clr-namespace:CompiledBindings" xmlns:g1="clr-namespace:WPFTest.Views">

    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="booleanToVisibilityConverter" />

        <BitmapImage x:Key="defaultImage" UriSource="/Images/download.png" />

        <DataTemplate x:Key="test1Template" DataType="viewModels:EntityViewModel">
            <Grid g0:DataTemplateBindings.Root="{Binding ElementName=window1}">
                <StackPanel>
                    <TextBlock x:Name="textBlock1" />
                    <TextBlock x:Name="textBlock2" />
                </StackPanel>
            <g0:DataTemplateBindings.Bindings><g1:MainWindow_DataTemplate0 /></g0:DataTemplateBindings.Bindings></Grid>
        </DataTemplate>
    </Window.Resources>

    <ScrollViewer>
        <StackPanel>
            <Image Width="200" x:Name="image1" />
            <TextBox x:Name="textBox1" />
            <TextBlock x:Name="textBlock3" />
            <TextBlock x:Name="textBlock4" />
            <TextBlock x:Name="textBlock5" />
            <TextBlock x:Name="textBlock6" />
            <TextBlock x:Name="textBlock7" />
            <TextBlock x:Name="textBlock8" />
            <TextBlock mx:DataType="{x:Null}" m:Text="{x:Bind InstanceFunction(1, 2)}" x:Name="textBlock9" />

            <Image Width="200" x:Name="image2" />

            <ListView x:Name="listView" ItemTemplate="{StaticResource test1Template}" m:SetVisible="{x:Bind ArrayProp.Length &gt; 0}" />
            <Button x:Name="button" CommandParameter="test" Content="TEST" />
        </StackPanel>
    </ScrollViewer>

</Window>
